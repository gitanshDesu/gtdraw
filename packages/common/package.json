{
    "name": "@gtdraw/common",
    "version": "1.0.0",
    "private": "true",
    "scripts": {
        "build": "tsc -b",
        "build:watch": "tsc -b"
    },
    "devDependencies": {
        "@gtdraw/typescript-config": "workspace:*",
        "@types/express": "^5.0.3",
        "@types/node": "^24.0.3",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "express": "^5.1.0",
        "zod": "^3.25.67"
    },
    "exports": {
