{
    "name": "@gtdraw/common",
    "version": "1.0.0",
    "private": "true",
    "scripts": {
        "build": "tsc -b",
        "build:watch": "tsc -b"
    },
    "devDependencies": {
        "@gtdraw/db": "workspace:*",
        "@gtdraw/typescript-config": "workspace:*",
        "@types/express": "^5.0.3",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^24.0.3",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.840.0",
        "@aws-sdk/s3-request-presigner": "^3.840.0",
        "bcryptjs": "^3.0.2",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2",
        "zod": "^3.25.67"
    },
    "exports": {
        "./*": {
            "types": "./src/zod/*.ts",
            "default": "./dist/zod/*.js"
        },
        "./utils/*": {
            "types": "./src/utils/*.ts",
            "default": "./dist/utils/*.js"
        },
        "./types/*": {
            "types": "./src/types/*.ts"
        },
        "./config": {
            "types": "./src/config/index.ts",
            "default": "./dist/config/index.js"
        }
    }
}